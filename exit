[32m+[m[32mfrom uuid import uuid4[m
[32m+[m[32mfrom datetime import datetime[m
[32m+[m[32mfrom app.database import Base[m
[32m+[m
[32m+[m[32mclass PromptRequest(Base):[m
[32m+[m[32m    __tablename__ = "PromptRequest"[m
[32m+[m
[32m+[m[32m    id = Column(UUID(as_uuid=True), primary_key=True, default=uuid4)[m
